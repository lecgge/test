<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/park_two_radius_two_angle"
        tools:context=".fragment.drivearea.ParkStartFragment">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/park_content_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <com.wisdplat.hmi.view.ParkSwitchView
                android:id="@+id/park_switch_button"
                android:layout_width="200dp"
                android:layout_height="25dp"
                android:contentDescription="@string/park_switch_view_content"
                app:bgColor="#5F6875"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.034"
                app:leftColor="#075CDB"
                app:rightColor="#075CDB"
                app:setChecked="true"
                app:textLeft="开始泊入"
                app:textLeftClickColor="#FFFFFF"
                app:textLeftColor="#FFFFFF"
                app:textRight="重新泊入"
                app:textRightClickColor="#FFFFFF"
                app:textRightColor="#FFFFFF"
                app:time="500"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView15"
                app:layout_constraintEnd_toEndOf="@+id/imageView15"
                app:layout_constraintStart_toStartOf="@+id/imageView15"
                app:srcCompat="@drawable/park_img_square_one" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="85dp"
                android:layout_height="55dp"
                android:layout_marginBottom="-20dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView9"
                app:layout_constraintEnd_toEndOf="@+id/imageView9"
                app:layout_constraintStart_toStartOf="@+id/imageView9"
                app:srcCompat="@drawable/park_img_square_car" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="85dp"
                android:layout_height="55dp"
                android:layout_marginEnd="-5dp"
                android:layout_marginBottom="-10dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageView17"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/park_img_square_car" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="85dp"
                android:layout_height="55dp"
                android:layout_marginBottom="-10dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="@+id/imageView11"
                app:layout_constraintStart_toStartOf="@+id/imageView11"
                app:srcCompat="@drawable/park_img_square_car" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView12"
                app:layout_constraintEnd_toEndOf="@+id/imageView12"
                app:layout_constraintStart_toStartOf="@+id/imageView12"
                app:srcCompat="@drawable/park_img_square_three" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.1"
                app:layout_constraintStart_toEndOf="@+id/imageView17"
                app:srcCompat="@drawable/park_img_square_four" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginBottom="-10dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView10"
                app:layout_constraintEnd_toEndOf="@+id/imageView10"
                app:layout_constraintStart_toStartOf="@+id/imageView10"
                app:srcCompat="@drawable/park_img_square_none"
                tools:ignore="ImageContrastCheck" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginBottom="-10dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView13"
                app:layout_constraintEnd_toEndOf="@+id/imageView13"
                app:layout_constraintStart_toStartOf="@+id/imageView13"
                app:srcCompat="@drawable/park_img_gray_car" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="75dp"
                android:layout_height="37dp"
                android:layout_marginBottom="-5dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toTopOf="@+id/imageView8"
                app:layout_constraintEnd_toEndOf="@+id/imageView8"
                app:layout_constraintStart_toStartOf="@+id/imageView8"
                app:srcCompat="@drawable/park_img_square_two" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginStart="60dp"
                android:layout_marginBottom="12dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintBottom_toBottomOf="@+id/imageView15"
                app:layout_constraintStart_toStartOf="@+id/imageView15"
                app:layout_constraintTop_toTopOf="@+id/imageView15"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/park_img_blue_line" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="130dp"
                android:layout_height="100dp"
                android:contentDescription="@string/park_img_content"
                app:layout_constraintEnd_toEndOf="@+id/park_switch_button"
                app:layout_constraintStart_toStartOf="@+id/park_switch_button"
                app:layout_constraintTop_toBottomOf="@+id/park_switch_button"
                app:srcCompat="@drawable/park_img_double_line" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_marginTop="5dp"
            android:contentDescription="@string/park_img_content"
            android:src="@drawable/park_img_park_car"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/park_content_constraint_layout" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
